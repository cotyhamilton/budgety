{
	"name": "budgety",
	"version": "1.0.0-next.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/cotyhamilton/budgety.git"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "yarn test:integration && yarn test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:e2e": "playwright test",
		"test:codegen": "playwright codegen http://localhost:4173/",
		"test:unit": "vitest run",
		"test:coverage": "vitest run --coverage",
		"change": "beachball change"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@square/svelte-store": "^1.0.17",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.25.1",
		"@testing-library/svelte": "^4.0.5",
		"@typescript-eslint/eslint-plugin": "^6.11.0",
		"@typescript-eslint/parser": "^6.9.1",
		"@vitest/coverage-v8": "^0.34.6",
		"@vlcn.io/crsqlite-wasm": "^0.15.2",
		"autoprefixer": "^10.4.16",
		"beachball": "^2.39.0",
		"bits-ui": "^0.9.8",
		"clsx": "^2.0.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-square-svelte-store": "^1.0.0",
		"eslint-plugin-svelte": "^2.30.0",
		"formsnap": "^0.4.1",
		"jsdom": "^22.1.0",
		"lucide-svelte": "^0.292.0",
		"postcss": "^8.4.31",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"radix-icons-svelte": "^1.2.1",
		"svelte": "^4.2.5",
		"svelte-check": "^3.4.3",
		"sveltekit-superforms": "^1.10.2",
		"tailwind-merge": "^2.0.0",
		"tailwind-variants": "^0.1.18",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.5.0",
		"vitest": "^0.34.6"
	},
	"type": "module",
	"dependencies": {}
}
